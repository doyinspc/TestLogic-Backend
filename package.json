{
  "name": "backend",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build": "babel www.js ",
    "pm2": "pm2",
    "gulp": "gulp",
    "start": "npm run build && nodemon  ./www.js",
    "start:staging": "pm2 start",
    "dev": "concurrently \"pm2 start\" \"gulp\"",
    "monitor": "pm2 monit",
    "reload-env": "pm2 reload --update-env ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js"
  },
  "dependencies": {
    "acorn": "^7.0.0",
    "async-middleware": "^1.2.1",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "browser-sync": "^2.26.7",
    "concurrently": "^5.1.0",
    "connect-redis": "^4.0.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "ejs": "~3.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "~4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-csscomb": "^3.1.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-html-prettify": "0.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-npm-dist": "^1.0.3",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-useref-plus": "0.0.8",
    "gulp-util": "^3.0.8",
    "gulp-wait": "0.0.2",
    "http-errors": "~1.7.3",
    "joi": "^14.3.1",
    "knex": "^0.20.9",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.1",
    "pino": "^5.16.0",
    "pm2": "^4.2.3",
    "postcss-flexbugs-fixes": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "eslint": "^6.1.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "pino-pretty": "^3.5.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.1.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "packages/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
